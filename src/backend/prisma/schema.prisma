// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  title     String
  content   String?
  published Boolean  @default(false)
  author    User     @relation(fields: [authorId], references: [id])
  authorId  Int
}

model Fab_Weld_Request {
  id     Int     @id @default(autoincrement())
  bio    String?
  user   User    @relation(fields: [userId], references: [id])
  userId Int     @unique
}

model Change_Request {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  age     Int?
  posts   Post[]
  profile Profile?
}

model Change {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  age     Int?
  posts   Post[]
  profile Profile?
}

model WBS_Element {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  age     Int?
  posts   Post[]
  profile Profile?
}

model Project {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  age     Int?
  posts   Post[]
  profile Profile?
}

model Work_Package {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  age     Int?
  posts   Post[]
  profile Profile?
}

model Description_Bullet {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  age     Int?
  posts   Post[]
  profile Profile?
}

